{
    "name": "napoleon/ipay88",
    "type": "library",
    "description": "ipay88 library",
    "keywords": ["ipay88", "payment gateway"],
    "homepage": "https://github.com/napoleon101392/ipay88",
    "authors": [{
        "name": "napoleon",
        "email": "napoleon101392@gmail.com",
        "homepage": "https://napoleon101392.github.io.com/me",
        "role": "Developer"
    }],
    "require": {
        "php": ">=5.5",
        "mockery/mockery": "^1.2",
        "vlucas/phpdotenv": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "IPay88\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IPay88\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
